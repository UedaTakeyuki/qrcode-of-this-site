{
  "name": "qrcode-of-this-site",
  "version": "1.0.2",
  "description": "vue component to show qrcode of this site",
  "author": "Dr. Takeyuki UEDA",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name qrcode-of-this-site --formats umd ./src/build-entry.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/qrcode-of-this-site.umd.js",
  "files": [
    "dist/*",
    "src/components/QRcode.vue",
    "src/build-entry.js"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "qrcode": "^1.4.4",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuetify": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "stylelint": "refer https://qiita.com/saio-th/items/111f6f5cc62f421cf045",
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
